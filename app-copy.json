{
  "expo": {
    "name": "Pocket Infoweb Extra",
    "slug": "PocketInfoweb-Extra",
    "description": "Find your tools inventory.",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.2.13",
    "icon": "./assets/images/icon.png",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/images/*", "assets/fonts/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "113",
      "bundleIdentifier": "com.helpfulconsultants.pocketinfowebextra",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": ["fetch"]
      }
    },
    "android": {
      "versionCode": 17
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0b4a76"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#3689b1"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "helpful-consultants",
          "project": "pocket-infoweb-extra"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "hooks": {
      "postPublish": []
    }
  }
}
